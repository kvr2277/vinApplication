<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.model.Address" table="address"
		catalog="vinoracle">
		<id name="addressId" type="int">
			<column name="ADDRESS_ID" />
			<generator class="sequence">
				<param name="sequence">address_seq</param>
			</generator>
		</id>
		<property name="addressLine1" type="string">
			<column name="ADDRESS_LINE_1" length="10" not-null="true"
				unique="true" />
		</property>
		<property name="addressLine2" type="string">
			<column name="ADDRESS_LINE_2" length="20" not-null="false"
				unique="true" />
		</property>
		<property name="city" type="string">
			<column name="CITY" length="10" not-null="false" unique="true" />
		</property>
		<property name="country" type="string">
			<column name="COUNTRY" length="20" not-null="true" unique="true" />
		</property>
		<property name="state" type="string">
			<column name="STATE" length="10" not-null="false" unique="true" />
		</property>
		<property name="zipcode" type="string">
			<column name="ZIPCODE" length="20" not-null="true" unique="true" />
		</property>
	</class>

</hibernate-mapping>